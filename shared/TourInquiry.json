{
  "name": "TourInquiry",
  "type": "object",
  "properties": {
    "destination_id": {
      "type": "string",
      "description": "ID of the destination"
    },
    "destination_name": {
      "type": "string",
      "description": "Name of the destination"
    },
    "full_name": {
      "type": "string",
      "description": "Customer's full name"
    },
    "email": {
      "type": "string",
      "format": "email",
      "description": "Customer's email"
    },
    "phone": {
      "type": "string",
      "description": "Customer's phone number"
    },
    "travel_date": {
      "type": "string",
      "format": "date",
      "description": "Preferred travel date"
    },
    "group_size": {
      "type": "number",
      "description": "Number of travelers"
    },
    "special_requirements": {
      "type": "string",
      "description": "Any special requirements or notes"
    },
    "budget_range": {
      "type": "string",
      "enum": [
        "under-10k",
        "10k-25k",
        "25k-50k",
        "above-50k"
      ],
      "description": "Budget range in INR"
    },
    "status": {
      "type": "string",
      "enum": [
        "pending",
        "contacted",
        "confirmed",
        "cancelled"
      ],
      "default": "pending",
      "description": "Inquiry status"
    }
  },
  "required": [
    "destination_name",
    "full_name",
    "email",
    "phone",
    "travel_date",
    "group_size"
  ]
}